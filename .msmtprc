# default values for all accounts
defaults
auth on
port 587
tls            on
tls_trust_file /Users/tminor/Misc/certs/ca-bundle.crt
logfile        ~/.msmtp.log

# Gmail
account        gmail
host           smtp.gmail.com
port           587
from           minort@gmail.com
user           minort
passwordeval   "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.passwd/minort.gpg"